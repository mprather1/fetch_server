{
  "name": "fetchserver",
  "version": "0.0.2",
  "description": "Server fetches data from other server",
  "main": "build/server.js",
  "repository": "https://github.com/mprather1/fetch_server",
  "author": "Michael Prather",
  "license": "MIT",
  "scripts": {
    "start": "SERVER_ADDRESS=http://localhost:8000 NODE_ENV=development PORT=8001 TZ='America/Chicago' node build/server.js",
    "dev": "SERVER_ADDRESS=http://localhost:8000 NODE_ENV=development PORT=8001 TZ='America/Chicago' nodemon build/server.js",
    "test": "PORT=8001 TZ='America/Chicago' NODE_ENV=test mocha build/test -t 10000",
    "build": "babel -d build src",
    "babel:watch": "babel --watch -d build src | ts '[%Y-%m-%d %H:%M:%S]'"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "chalk": "^1.1.3",
    "express": "^4.15.2",
    "got": "^6.7.1",
    "morgan": "^1.8.1",
    "pg": "^6.1.5",
    "pg-promise": "^5.6.4",
    "url": "^0.11.0",
    "winston-color": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-es2015-node6": "^0.4.0",
    "babel-preset-latest": "^6.24.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "mocha": "^3.2.0",
    "standard": "^9.0.2"
  },
  "standard": {
    "globals": [
      "it",
      "describe",
      "beforeEach",
      "afterEach",
      "before",
      "after",
      "Backbone",
      "_"
    ]
  },
  "engines": {
    "node": ">=6"
  }
}
